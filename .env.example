# Firebase Configuration (클라이언트)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Social Login Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_KAKAO_APP_KEY=your_kakao_javascript_key
VITE_NAVER_CLIENT_ID=your_naver_client_id
VITE_NAVER_CLIENT_SECRET=your_naver_client_secret
VITE_NAVER_CALLBACK_URL=http://localhost:5173/auth/naver/callback

# Server Configuration
PORT=5000
NODE_ENV=development

# Firebase Admin SDK (서버)
FIREBASE_SERVICE_ACCOUNT_PATH=./serviceAccountKey.json
